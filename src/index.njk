{# This page is built with nunjucks! #}

{% set title = 'Home' %}
{% extends "layout.njk" %}

{% block body %}
  <main class="container py-8">
    <h1>This is the {{ title | lower }} page.</h1>
    {%for project in collections.projects%}
      <a href="{{project.url}}">{{project.data.title}}</a> 
      <img src="{{project.images[0].image}}">
    {%endfor%}
  </main>
{% endblock %}
